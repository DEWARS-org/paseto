{
  "name": "paseto-ts",
  "version": "0.0.1",
  "description": "PASETO v4 (encrypt, decrypt, sign & verify) in TypeScript",
  "type": "module",
  "types": "./dist/index.d.ts",
  "keywords": [
    "decrypt",
    "encrypt",
    "sign",
    "verify",
    "paseto",
    "v4",
    "local",
    "public",
    "typescript"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "tsm node_modules/uvu/bin.js tests",
    "test:coverage": "c8 --include=src npm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov"
  },
  "homepage": "https://github.com/miunau/paseto-ts",
  "repository": "miunau/paseto-ts",
  "author": "miunau <miunau+npm@miunau.com> (https://www.miunau.com)",
  "license": "MIT",
  "devDependencies": {
    "tsm": "^2.3.0",
    "typescript": "^4.9.4",
    "uvu": "^0.5.6",
    "c8": "^7.12.0"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "dependencies": {
    "@stablelib/blake2b": "^1.0.1",
    "@stablelib/ed25519": "^1.0.3",
    "@stablelib/xchacha20": "^1.0.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    },
    "v4": {
      "types": "./dist/v4/index.d.ts",
      "import": "./dist/v4/index.js"
    },
    "v4/decrypt": {
      "types": "./dist/v4/decrypt.d.ts",
      "import": "./dist/v4/decrypt.js"
    },
    "v4/encrypt": {
      "types": "./dist/v4/encrypt.d.ts",
      "import": "./dist/v4/encrypt.js"
    },
    "v4/key": {
      "types": "./dist/v4/key.d.ts",
      "import": "./dist/v4/key.js"
    },
    "v4/sign": {
      "types": "./dist/v4/sign.d.ts",
      "import": "./dist/v4/sign.js"
    },
    "v4/verify": {
      "types": "./dist/v4/verify.d.ts",
      "import": "./dist/v4/verify.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist"
      ],
      "lib": [
        "./dist/lib"
      ],
      "v4": [
        "./dist/v4"
      ],
      "v4/decrypt": [
        "./dist/v4"
      ],
      "v4/encrypt": [
        "./dist/v4"
      ],
      "v4/key": [
        "./dist/v4"
      ],
      "v4/sign": [
        "./dist/v4"
      ],
      "v4/verify": [
        "./dist/v4"
      ]
    }
  }
}
